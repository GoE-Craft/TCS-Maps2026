{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "tcs_vlg:celebration_floor_particles",
			"basic_render_parameters": {
				"material": "particles_add",
				"texture": "textures/tcs/vlg/particles/glow"
			}
		},
		"curves": {
			"variable.bounce": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [10, -15, -15, -15]
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 80
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1
			},
			"minecraft:emitter_shape_sphere": {
				"offset": [0, 1.5, -2],
				"radius": 1.5,
				"surface_only": true,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 2
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random(-180, 180)"
			},
			"minecraft:particle_initial_speed": "math.random(2,14)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, "(v.bounce * v.particle_lifetime )", 0],
				"linear_drag_coefficient": 1
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["(0.1* v.particle_lifetime) * v.particle_random_1", "(0.1 * v.particle_lifetime) * v.particle_random_1"],
				"facing_camera_mode": "rotate_xyz"
			},
			"minecraft:particle_motion_collision": {
				"collision_drag": 0.2,
				"coefficient_of_restitution": 1,
				"collision_radius": 0.2
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": " v.emitter_random_1 / (v.particle_age + v.particle_random_1)",
					"gradient": {
						"0.0": "#FFFF0000",
						"0.23": "#FFBC00FF",
						"0.5": "#FF0085FF",
						"0.78": "#FF1FFF11",
						"1.0": "#FFFFF200"
					}
				}
			}
		}
	}
}